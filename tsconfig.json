{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react-native",
		"lib": ["es6", "ES2017"],
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"noUncheckedIndexedAccess": true,
		"incremental": true,
		"pretty": true
	},
	"exclude": [
		"node_modules",
		"@types"
	]
}
